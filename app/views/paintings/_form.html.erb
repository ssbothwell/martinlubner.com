<% form_for @painting, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
    <%= f.file_field :photo %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
