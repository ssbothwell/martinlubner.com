<% form_for @painting, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
    <%= f.file_field :photo %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
	  <%= f.label :height %>
    <%= f.text_field :height %>
    <%= f.label :width %>
    <%= f.text_field :width %>
  </p>
  <p>
    <%= f.label :year %><br />
    <%= f.text_field :year %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
